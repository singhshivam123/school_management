<div class="content-wrapper">
  <div class="page-header">
    <h3 class="page-title"> Student Details </h3>
    <nav aria-label="breadcrumb">
      <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="<%= admin_students_path %>">Student Tables</a></li>
        <li class="breadcrumb-item active" aria-current="page">Show Student Details</li>
      </ol>
    </nav>
  </div>
  <div class="row">
    <div class="col-md-12 grid-margin">
      <div class="card">
        <div class="card-body">
          <h4 class="card-title">Student Details</h4>
          <div class="progress" style="background: #ffffff;">
            <div class="progress-bar bg-primary" style="height: 3px; width: 90">
            </div>
          </div>
          <div class="row">
            <div class="col-md-6">
              <div  style="text-align: center; padding-top: 9px; border: 0px; background-color: beige;"><p class="text-info">First name:</p></div>
              <div  style="text-align: center; padding-top: 9px; border: 0px; background-color: beige;"><p class="text-info">Middle name:</p></div>
              <div  style="text-align: center; padding-top: 9px; border: 0px; background-color: beige;"><p class="text-info">Last name:</p></div>
              <div  style="text-align: center; padding-top: 9px; border: 0px; background-color: beige;"><p class="text-info">Date of birth:</p></div>
              <div  style="text-align: center; padding-top: 9px; border: 0px; background-color: beige;"><p class="text-info">Father name:</p></div>
              <div  style="text-align: center; padding-top: 9px; border: 0px; background-color: beige;"><p class="text-info">Join date:</p></div>
              <div  style="text-align: center; padding-top: 9px; border: 0px; background-color: beige;"><p class="text-info">Leaving date:</p></div>
              <div  style="text-align: center; padding-top: 9px; border: 0px; background-color: beige;"><p class="text-info">Mobile:</p></div>
              <div  style="text-align: center; padding-top: 9px; border: 0px; background-color: beige;"><p class="text-info">Email:</p></div>
            </div>
            <div class="col-md-6">
              <div style="text-align: center; padding-top: 9px; border: 0px; background-color: aquamarine;"><p><%= @student.first_name %></p></div>
              <div style="text-align: center; padding-top: 9px; border: 0px; background-color: aquamarine;"><p><%= @student.middle_name %></p></div>
              <div style="text-align: center; padding-top: 9px; border: 0px; background-color: aquamarine;"><p><%= @student.last_name %></p></div>
              <div style="text-align: center; padding-top: 9px; border: 0px; background-color: aquamarine;"><p><%= @student.date_of_birth %></p></div>
              <div style="text-align: center; padding-top: 9px; border: 0px; background-color: aquamarine;"><p><%= @student.father_name %></p></div>
              <div style="text-align: center; padding-top: 9px; border: 0px; background-color: aquamarine;"><p><%= @student.join_date %></p></div>
              <div style="text-align: center; padding-top: 9px; border: 0px; background-color: aquamarine;"><p><%= @student.leaving_date %></p></div>
              <div style="text-align: center; padding-top: 9px; border: 0px; background-color: aquamarine;"><p><%= @student.mobile %></p></div>
              <div style="text-align: center; padding-top: 9px; border: 0px; background-color: aquamarine;"><p><%= @student.email %></p></div>
            </div>
          </div>&nbsp;
          <div class="text-center">
            <%= button_to "Edit", edit_admin_student_path(@student), method: :get, class: "btn btn-outline-primary" %>
            <%= button_to "Back", admin_students_path, method: :get, class: "btn btn-outline-primary "  %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
