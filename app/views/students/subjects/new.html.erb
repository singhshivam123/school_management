<div class="content-wrapper">
  <h2>Add New Subject</h2>
  <%= form_with(model: @subject, local: true, url: students_subjects_path) do |form| %>

    <div class="field">
      <%= form.label :name %>
      <%= form.text_field :name, class: 'form-control' %>
    </div>&nbsp;

    <div class="actions">
      <%= form.submit class: 'btn btn-primary' %>
    </div>
  <% end %>

</div>
